#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:32
msgid "<!--"
msgstr "<!--"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:33
msgid "  var LastProgID = 'LastProgID';"
msgstr "  var LastProgID = 'LastProgID';"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:35
msgid "  var URLPath = \"/\""
msgstr "  var URLPath = \"/\""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:36
msgid "  function Go(s)"
msgstr "  function Go(s)"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:37
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:48
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:60
msgid "  {"
msgstr "  {"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:38
msgid "    // Save the progid in a cookie"
msgstr "    // Save the progid in a cookie"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:39
msgid "    var exp = new Date();"
msgstr "    var exp = new Date();"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:40
msgid "    exp.setTime(exp.getTime() + (7 * 24 * 60 * 60 * "
"1000));"
msgstr "    exp.setTime(exp.getTime() + (7 * 24 * 60 * 60 * "
"1000));"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:41
msgid "    document.cookie = LastProgID + '=' + s "
"+"
msgstr "    document.cookie = LastProgID + '=' + s "
"+"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:42
msgid "    '; expires=' + exp.toGMTString();"
msgstr "    '; läuft ab=' + exp.toGMTString();"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:43
msgid "    // jump using the progid"
msgstr "    // jump using the progid"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:44
msgid "    window.location.href = URLPath + "
"s;"
msgstr "    window.location.href = URLPath + "
"s;"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:45
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:57
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:64
msgid "  }"
msgstr "  }"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:47
msgid "  function GetCookie(sName)"
msgstr "  function GetCookie(sName)"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:49
msgid "    var aCookie = document.cookie.split(\"; "
"\");"
msgstr "    var aCookie = document.cookie.split(\"; "
"\");"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:50
msgid "    for (var i=0; i <aCookie.length; "
"i++)"
msgstr "    for (var i=0; i <aCookie.length; "
"i++)"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:51
msgid "    {"
msgstr "    {"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:52
msgid "      var aCrumb = aCookie[i].split(\"=\");"
msgstr "      var aCrumb = aCookie[i].split(\"=\");"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:53
msgid "      if (sName == aCrumb[0])"
msgstr "      if (sName == aCrumb[0])"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:54
msgid "        return unescape(aCrumb[1]);"
msgstr "        return unescape(aCrumb[1]);"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:55
msgid "    }"
msgstr "    }"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:56
msgid "    return null;"
msgstr "    return null;"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:59
msgid "  function init()"
msgstr "  function init()"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:61
msgid "    var s = GetCookie(LastProgID);"
msgstr "    var s = GetCookie(LastProgID);"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:62
msgid "    if (s != null)"
msgstr "    if (s != null)"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:63
msgid "      document.F.S.value = s;"
msgstr "      document.F.S.value = s;"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:65
msgid "// -->"
msgstr "// -->"

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:66
msgid "</script>"
msgstr "</script>"

