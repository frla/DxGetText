#: SvrInfoModule.dfm:64
msgid "  }"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:47
msgid "  function GetCookie(sName)"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:49
msgid "    var aCookie = document.cookie.split(\"; "
"\");"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:50
msgid "    for (var i=0; i <aCookie.length; "
"i++)"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:51
msgid "    {"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:52
msgid "      var aCrumb = aCookie[i].split(\"=\");"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:53
msgid "      if (sName == aCrumb[0])"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:54
msgid "        return unescape(aCrumb[1]);"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:55
msgid "    }"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:56
msgid "    return null;"
msgstr ""

