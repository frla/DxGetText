#: SvrInfoModule.dfm:39
msgid "    var exp = new Date();"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:40
msgid "    exp.setTime(exp.getTime() + (7 * 24 * 60 * 60 * "
"1000));"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:41
msgid "    document.cookie = LastProgID + '=' + s "
"+"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:42
msgid "    '; expires=' + exp.toGMTString();"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:43
msgid "    // jump using the progid"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:44
msgid "    window.location.href = URLPath + "
"s;"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:45
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
