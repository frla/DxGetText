#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:49
msgid "    var aCookie = document.cookie.split(\"; "
"\");"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:50
msgid "    for (var i=0; i <aCookie.length; "
"i++)"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:51
msgid "    {"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:52
msgid "      var aCrumb = aCookie[i].split(\"=\");"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:53
msgid "      if (sName == aCrumb[0])"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:54
msgid "        return unescape(aCrumb[1]);"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:55
msgid "    }"
msgstr ""

#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.xfm:56
msgid "    return null;"
